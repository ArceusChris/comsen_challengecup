<?xml version="1.0"?>
<launch>
    <!-- YOLO11推理节点 - iris机型示例 -->
    <node name="yolo11_inference_iris" pkg="your_package" type="yolo11_inference.py" output="screen">
        <param name="aircraft_type" value="iris" />
        <param name="model_path" value="models/yolo11n_original.pt" />
        <param name="confidence_threshold" value="0.5" />
        <param name="iou_threshold" value="0.45" />
        <param name="publish_pointcloud" value="true" />
        <param name="pointcloud_frame_id" value="map" />
        <param name="max_points_per_cloud" value="1000" />
    </node>
    
